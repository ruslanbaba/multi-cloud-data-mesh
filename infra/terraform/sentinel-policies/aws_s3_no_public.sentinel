import "tfplan/v2" as tfplan

main = rule {
  all tfplan.resource_changes as rc {
    rc.type is "aws_s3_bucket_public_access_block" implies
    rc.change.after.block_public_acls and rc.change.after.block_public_policy and rc.change.after.ignore_public_acls and rc.change.after.restrict_public_buckets
  }
}
